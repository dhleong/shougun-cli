{
  "name": "@shougun/cli",
  "description": "Take command of your media",
  "version": "0.1.1",
  "author": "Daniel Leong",
  "bin": {
    "shougun": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/dhleong/shougun-cli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "cli-progress": "^3.4.0",
    "cli-ux": "^5.3.3",
    "debug": "^4.1.1",
    "inquirer": "^7.0.0",
    "msgpack-rpc-lite": "^1.0.2",
    "node-ssdp": "^4.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.5",
    "@types/chai-subset": "^1.3.3",
    "@types/cli-progress": "^1.8.1",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^8.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.12",
    "@types/node-ssdp": "^3.3.0",
    "@types/range-parser": "^1.2.3",
    "@types/request-promise-native": "^1.0.17",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "globby": "^10.0.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dhleong/shougun-cli#readme",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "shougun",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhleong/shougun-cli.git"
  },
  "scripts": {
    "build": "tsc -p .",
    "check": "npm run lint && npm run build && npm run test",
    "lint": "tslint -p .",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepare": "npm run build",
    "prepublishOnly": "npm run check",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*-test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
