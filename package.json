{
  "name": "@shougun/cli",
  "description": "Take command of your media",
  "version": "0.2.0",
  "author": "Daniel Leong",
  "bin": {
    "shougun": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/dhleong/shougun-cli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "cli-progress": "^3.8.2",
    "cli-ux": "^5.4.9",
    "debug": "^4.1.1",
    "inquirer": "^7.3.3",
    "msgpack-rpc-lite": "^1.0.2",
    "node-ssdp": "^4.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.12",
    "@types/chai-subset": "^1.3.3",
    "@types/cli-progress": "^3.7.0",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^7.3.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.26",
    "@types/node-ssdp": "^3.3.1",
    "@types/range-parser": "^1.2.3",
    "@types/request-promise-native": "^1.0.17",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "globby": "^11.0.1",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dhleong/shougun-cli#readme",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "shougun",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhleong/shougun-cli.git"
  },
  "scripts": {
    "build": "tsc -p .",
    "check": "npm run lint && npm run build && npm run test",
    "lint": "tslint -p .",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepare": "npm run build",
    "prepublishOnly": "npm run check",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*-test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
